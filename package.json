{
  "name": "react-template",
  "version": "1.1.0",
  "description": "react template.",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development BASE_ENV=development webpack serve -c build/webpack.dev.ts",
    "test": "cross-env NODE_ENV=development BASE_ENV=test webpack serve -c build/webpack.dev.ts",
    "pre": "cross-env NODE_ENV=development BASE_ENV=pre webpack serve -c build/webpack.dev.ts",
    "prod": "cross-env NODE_ENV=development BASE_ENV=production webpack serve -c build/webpack.dev.ts",
    "build:dev": "cross-env NODE_ENV=production BASE_ENV=development webpack -c build/webpack.prod.ts",
    "build:test": "cross-env NODE_ENV=production BASE_ENV=test webpack -c build/webpack.prod.ts",
    "build:pre": "cross-env NODE_ENV=production BASE_ENV=pre webpack -c build/webpack.prod.ts",
    "build:prod": "cross-env NODE_ENV=production BASE_ENV=production webpack -c build/webpack.prod.ts",
    "build:analy": "cross-env NODE_ENV=production BASE_ENV=production webpack -c build/webpack.analy.ts",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,stylus,html,md}\"",
    "lint:md": "npx markdownlint --config .markdownlint.js --fix .",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "pre-check": "tsc && npx lint-staged",
    "commit": "git pull && git add -A && git-cz && git push",
    "release": "standard-version"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^5.18.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "echarts": "^5.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/node": "^20.12.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "commitizen": "4.3.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.37.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cz-git": "1.5.3",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "express": "^4.19.2",
    "glob-all": "^3.3.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "markdownlint-cli": "^0.41.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.2",
    "purgecss-webpack-plugin": "^6.0.0",
    "react-refresh": "^0.14.2",
    "resize-observer-polyfill": "^1.5.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "standard-version": "^9.5.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.6.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "terser-webpack-plugin": "^5.3.10",
    "thread-loader": "^4.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.10.0",
    "webpackbar": "^6.0.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "pnpm run lint:eslint",
      "pnpm run lint:prettier"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}